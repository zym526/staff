page{height: 100%;overflow-x: hidden;}
.mmzwf { height : 100rpx ; width: 100% ; }
.footer { width: 100% ; overflow:  hidden ; height : 100rpx ; border-top : 1rpx solid #dddddd; position: fixed ;bottom : 0 ; font-size: 26rpx ; line-height: 100rpx ; z-index: 1000 ; background: #fff ;}
.footer view { width: 50% ; float: left ; text-align: center ; color : #666666 ;  }
.footer .active { color : #f03c3b ; }
.red_bg_color{
    background: #D9161C;
}

.tabbar_outer{
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0,0,0,0.1);
  background-color: white;
  z-index: 999;
}
.tabbar_only{
  width: 50%;
  padding-top: 20rpx;
  text-align: center;
  font-size: 22rpx;
  line-height: 30rpx;
  color: #999999;
  padding-bottom: 10rpx;
}
.tabbar_only image{
  width: 40rpx;
  height: 40rpx;
}



